{
  "name": "proboard-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "server": "nodemon server/index.ts",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "rb-image-dev": "docker build -t proboard/nextjs-image -f Dockerfile-dev .",
    "docker-dev": "docker-compose -f docker-compose-dev.yml up",
    "docker-stop": "docker-compose -f docker-compose-dev.yml stop",
    "docker-dev-d": "docker-compose -f docker-compose-dev.yml up -d --build --force-recreate",
    "docker-rb": "docker-compose -f docker-compose-dev.yml up --build --force-recreate"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@prisma/client": "^4.12.0",
    "@types/node": "18.15.8",
    "@types/react": "18.0.29",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "18.0.11",
    "express": "^4.18.2",
    "next": "13.2.4",
    "next-auth": "^4.20.1",
    "react": "18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "18.2.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "typescript": "5.0.2",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.21",
    "prisma": "^4.12.0",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1"
  }
}
